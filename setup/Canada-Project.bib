isbn = {0123456789},
issn = {14321106},
journal = {Experimental Brain Research},
keywords = {Cross-education,Neuroplasticity,Rehabilitation,Strength training,Stroke},
number = {7},
pages = {2009--2021},
pmid = {29730752},
publisher = {Springer Berlin Heidelberg},
title = {{Unilateral wrist extension training after stroke improves strength and neural plasticity in both arms}},
url = {http://dx.doi.org/10.1007/s00221-018-5275-6},
volume = {236},
