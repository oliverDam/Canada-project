berg balance scale (BBS)
chedoke-McMaster stroke assessment (CMSA)
gait speed
montreal cognitive assessment (MoCA)},
author = {Wong, Jennifer S. and Brooks, Dina and Inness, Elizabeth L. and Mansfield, Avril},
doi = {10.1016/j.jstrokecerebrovasdis.2016.03.017},
file = {:Users/oliver/Downloads/1-s2.0-S1052305716001592-main.pdf:pdf},
isbn = {1052-3057},
issn = {15328511},
journal = {Journal of Stroke and Cerebrovascular Diseases},
keywords = {Stroke,accidental falls,recovery,rehabilitation},
