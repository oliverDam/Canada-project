isbn = {0021-9290 (Print)$\backslash$n0021-9290 (Linking)},
issn = {00219290},
journal = {Journal of Biomechanics},
keywords = {Compression,Force-sensing resistors,Hysteresis,Shear sensitivity},
number = {16},
pages = {3492--3495},
pmid = {19019374},
title = {{A technique for conditioning and calibrating force-sensing resistors for repeatable and reliable measurement of compressive force}},
